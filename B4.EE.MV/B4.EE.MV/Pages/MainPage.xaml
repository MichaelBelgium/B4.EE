<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:B4.EE.MV"
             x:Class="B4.EE.MV.Pages.MainPage">

    <StackLayout>
        <Entry Placeholder="City" Text="{Binding City}" />
        <Button Text="Search" Command="{Binding GetRequest}"/>
        <Button Text="Search gps" Command="{Binding GetRequestGps}" />
    </StackLayout>

</ContentPage>
